<?xml version='1.0' encoding='utf-8'?>
<widget id="nl.kabisa.buurkracht" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Buurkracht</name>
    <description />
    <content src="index.html" />
    <access origin="*" />
